{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-2",
    "flow"
  ],
  "plugins": [
    "transform-regenerator",
    "transform-async-to-generator",
    "transform-decorators-legacy"
    // "react-hot-loader/babel", // better use webpack way
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
