{
  "name": "react-bootstrap-webpack-starter",
  "version": "5.0.0",
  "description": "react js + bootstrap + webpack starter",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-bootstrap-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-bootstrap-webpack-starter/issues"
  },
  "engines": {
    "node": ">=8.9.1",
    "npm": ">=5.5.1",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "cross-env NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "rimraf docs/public/assets/ && cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "rimraf docs/public/assets/ && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js ",
    "start": "cross-env NODE_ENV=dev node server.hot.reload",
    "serve-dev": "npm run dev && nodemon ./src/server/index.js",
    "serve-prod": "npm run prod && node ./src/server/index.js"
  },
  "keywords": [
    "react",
    "react 16",
    "ES6",
    "ES7",
    "ES2015",
    "ES2016",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "hot reload",
    "webpack 3",
    "webpack",
    "react-router",
    "react-router 4",
    "react-router v4"
  ],
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
